{
  "name": "@gxmari007/vite-plugin-eslint",
  "version": "1.0.2",
  "description": "A vite eslint plugin.",
  "author": "James Gao <gaoxiang.ooxx@gmail.com>",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "npm run build -- --watch",
    "example:dev": "npm -C example run dev",
    "build": "tsup src/index.ts --dts --format esm,cjs",
    "commit": "cz",
    "prepublishOnly": "npm run build",
    "release": "npx bumpp --commit --tag --push && npm publish"
  },
  "license": "MIT",
  "keywords": [
    "vite",
    "plugin",
    "eslint"
  ],
  "homepage": "https://github.com/gxmari007/vite-plugin-eslint",
  "repository": "git+https://github.com:gxmari007/vite-plugin-eslint.git",
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@rollup/pluginutils": "^4.1.0",
    "eslint": "^7.18.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@types/eslint": "^7.2.6",
    "@types/node": "^14.14.21",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "^2.2.1",
    "rollup": "^2.38.0",
    "tsup": "^3.12.0",
    "typescript": "^4.1.3",
    "vite": "^2.0.0-beta.50"
  },
  "peerDependencies": {
    "rollup": "^2.38.0",
    "vite": "^2.0.0-beta.30"
  }
}
